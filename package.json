{
	"name": "@ctx-core/dom",
	"version": "11.0.8",
	"description": "ctx-core dom",
	"keywords": [
		"ctx-core",
		"dom"
	],
	"homepage": "https://github.com/ctx-core/dom#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/dom/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/dom/issues"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"type": "module",
	"main": "./dist/index.js",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		"require": "./dist/index.cjs",
		"import": "./dist/index.js"
	},
	"scripts": {
		"build": "npm run clean && npm run copy-mjs && npm run compile",
		"clean": "rm -f tsconfig.tsbuildinfo && rm -f src/**/*.js && rm -f src/**/*.d.ts && rm -f src/**/*.map",
		"compile": "./node_modules/.bin/tsc -b .",
		"copy-mjs": "cd dist && echo \"require = require('esm')(module)\nmodule.exports = require('./index.js')\" > index.cjs",
		"exec": "$@",
		"prepublishOnly": "npm run build"
	},
	"dependencies": {
		"@ctx-core/data": "workspace:^7.0.7",
		"@ctx-core/function": "workspace:^20.0.6",
		"@ctx-core/html": "workspace:^11.0.7",
		"@ctx-core/object": "workspace:^21.0.6",
		"@ctx-core/store": "workspace:^27.0.5",
		"esm": "^3.2.25"
	},
	"devDependencies": {
		"typescript": "^4.3.5"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	}
}
